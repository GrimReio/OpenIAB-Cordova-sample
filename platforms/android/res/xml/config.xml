<?xml version='1.0' encoding='utf-8'?>
<widget id="org.onepf.openiab.cordova.sample" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidLaunchMode" value="singleTop" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="OpenIabCordovaPlugin">
        <param name="android-package" value="org.onepf.openiab.cordova.OpenIabCordovaPlugin" />
    </feature>
    <name>CordovaSample</name>
    <description>
        A sample OpenIAB application for Cordova.
    </description>
    <content src="index.html" />
    <access origin="*" />
</widget>
